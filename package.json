{
  "name": "claiq-bags-backend",
  "version": "1.0.0",
  "description": "Backend API for Laiq Bags e-commerce website",
  "main": "server.js",
  "scripts": {
    "render-build": "npm install && node scripts/generate-sitemap.js",
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "seed": "node scripts/seedData.js",
    "build:css": "tailwindcss -i ./css/tailwind.css -o ./css/styles.css --watch",
    "build:css:prod": "tailwindcss -i ./css/tailwind.css -o ./css/styles.css --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "monitor": "node scripts/monitor-server.js",
    "quick-fix": "node scripts/quick-fix.js",
    "restart": "pkill -f 'node server.js' && npm start",
    "test-order": "node scripts/test-order-display.js",
    "test-color": "node scripts/test-color-display.js",
    "test-auth": "node scripts/test-auth-switching.js",
    "prod": "NODE_ENV=production node server.js",
    "postinstall": "node scripts/generate-sitemap.js"
  },
  "keywords": [
    "e-commerce",
    "bags",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Laiq Bags",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "mongoose-paginate-v2": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "rate-limit-redis": "^4.2.1",
    "razorpay": "^2.9.6",
    "redis": "^5.7.0",
    "sharp": "^0.33.5",
    "stripe": "^13.11.0",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}